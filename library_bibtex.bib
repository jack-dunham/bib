  local-url     = {/Users/JackDunham/OneDrive - University College London/Library/Article/S/Sweke2015_Sweke;Sinayskiy;et al_Universal simulation of Markovian open quantum systems_10.1103-physreva.91.062308_0.pdf},
  pdf           = {:Article/S/Sweke2015 - Sweke, Sinayskiy et al - Universal Simulation of Markovian Open Quantum Systems - 10.1103_physreva.91.062308.pdf:PDF},
