	journal = {Physical Review B},
	month = {Sep},
	number = {12},
	publisher = {American Physical Society (APS)},
	title = {Exciton-polaritons in flatland: Controlling flatband properties in a Lieb lattice},
	url = {http://dx.doi.org/10.1103/PhysRevB.102.121302},
	volume = {102},
	year = {2020},
	Bdsk-File-1 = {},
