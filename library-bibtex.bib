  local-url     = {/Users/JackDunham/OneDrive - University College London/Library/Article/L/Li2017_Li;Benjamin_Efficient Variational Quantum Simulator Incorporating Active Error Minimization_10.1103-physrevx.7.021050_0.pdf},
  pdf           = {:Article/L/Li2017 - Li, Benjamin - Efficient Variational Quantum Simulator Incorporating Active Error Minimization - 10.1103_physrevx.7.021050.pdf:PDF},
  publisher     = {American Physical Society ({APS})},
}

