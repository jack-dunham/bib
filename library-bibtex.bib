  local-url     = {/Users/JackDunham/OneDrive - University College London/Library/Article/P/Peruzzo2014_Peruzzo;McClean;et al_A variational eigenvalue solver on a photonic quantum processor_10.1038-ncomms5213_0.pdf},
  pdf           = {:Article/P/Peruzzo2014 - Peruzzo, McClean et al - A Variational Eigenvalue Solver on a Photonic Quantum Processor - 10.1038_ncomms5213.pdf:PDF},
  publisher     = {Springer Science and Business Media {LLC}},
