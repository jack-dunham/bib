  groups        = {Ansatz; Imported casestudy1.bib; Case Study 1},
  local-url     = {/Users/JackDunham/OneDrive - University College London/Library/Article/R/Romero2018_Romero;Babbush;et al_Strategies for quantum computing molecular energies using the unitary coupled cluster ansatz_10.1088-2058-9565-aad3e4_0.pdf},
  pdf           = {:Article/R/Romero2018 - Romero, Babbush et al - Strategies for Quantum Computing Molecular Energies Using the Unitary Coupled Cluster Ansatz - 10.1088_2058-9565_aad3e4.pdf:PDF},
