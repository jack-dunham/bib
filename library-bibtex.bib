@Article{Wu2012,
  author        = {Zhen Wu},
  journal       = {Chin. Phys. Lett.},
  title         = {Mocking up a Dephasing Channel with a Minimal-Sized Environment},
  year          = {2012},
  month         = aug,
  number        = {8},
  pages         = {080304},
