  pages     = {052129},
  volume    = {97},
  abstract  = {We develop a real-time full configuration-interaction quantum Monte Carlo approach to model driven-dissipative open quantum systems with Markovian system-bath coupling. The method enables stochastic sampling of the Liouville–von Neumann time evolution of the density matrix thanks to a massively parallel algorithm, thus providing estimates of observables on the nonequilibrium steady state. We present the underlying theory and introduce an initiator technique and importance sampling to reduce the statistical error. Finally, we demonstrate the efficiency of our approach by applying it to the driven-dissipative two-dimensional XYZ spin-12 model on a lattice.},
  doi       = {10.1103/PhysRevA.97.052129},
  file      = {Full Text PDF:https\://journals.aps.org/pra/pdf/10.1103/PhysRevA.97.052129:application/pdf},
  publisher = {American Physical Society},
  url       = {https://link.aps.org/doi/10.1103/PhysRevA.97.052129},
  urldate   = {2021-08-31},
}

@Article{Weimer2021,
  author    = {Weimer, Hendrik and Kshetrimayum, Augustine and Orús, Román},
  journal   = {Reviews of Modern Physics},
  title     = {Simulation methods for open quantum many-body systems},
  year      = {2021},
  month     = mar,
  number    = {1},
  pages     = {015008},
  volume    = {93},
  abstract  = {Coupling a quantum many-body system to an external environment dramatically changes its dynamics and offers novel possibilities not found in closed systems. Of special interest are the properties of the steady state of such open quantum many-body systems, as well as the relaxation dynamics toward the steady state. However, new computational tools are required to simulate open quantum many-body systems, as methods developed for closed systems cannot be readily applied. Several approaches to simulating open many-body systems are reviewed, and advances made in recent years toward the simulation of large system sizes are pointed out.},
  doi       = {10.1103/RevModPhys.93.015008},
  file      = {Full Text PDF:https\://journals.aps.org/rmp/pdf/10.1103/RevModPhys.93.015008:application/pdf},
  publisher = {American Physical Society},
  url       = {https://link.aps.org/doi/10.1103/RevModPhys.93.015008},
  urldate   = {2021-08-31},
}

@Unpublished{Lledo2021,
  archiveprefix = {arxiv},
  author        = {Lled{\'{o}}, Crist{\'{o}}bal and Carusotto, Iacopo and Szyma{{\'{n}}}ska, Marzena H.},
  title         = {Spontaneous vortex lattices in polariton condensates in the lowest {Landau} level of a strained honeycomb lattice},
  month         = mar,
  year          = {2021},
  abstract      = {Photonic materials are a rapidly growing platform for studying condensed matter physics with light. The exquisite capability of state preparation and detection, as well as the engineering of lattice geometries, is allowing us to learn about the relation between microscopic dynamics and macroscopic properties. Here we study Bose-Einstein condensation of exciton-polaritons into a lowest Landau level of a strained honeycomb lattice of semiconductor micropillars. The strain induces a pseudomagnetic field which has opposite sign in each Dirac valley. With a suitably designed sublattice-asymmetry of the incoherent pump, we find that the condensate spontaneously breaks the valley symmetry and polaritons accumulate in the lowest Landau level of a given valley. For growing condensate sizes, the interplay of the pseudomagnetic field and the quantisation of circulation in the condensate leads to the formation of a vortex array in analogy with an ultracold atomic superfluid in a rapidly rotating trap. We explain how this peculiar non-equilibrium stationary state -- which takes place in the middle of the energy spectrum -- results from a dynamical balance of incoherent pump and losses and is stabilised by the sublattice polarisation of the lowest Landau level. Our results offer a new platform where to study the exciting physics of arrays of quantised vortices and pave the way to explore the transition from a vortex-dominated phase to the fractional quantum Hall regime.},
  annote        = {Comment: 9 pages, 7 figures. Comments are very welcome},
  eprint        = {2103.07509},
  file          = {:Lledo2021.Spontaneous vortex lattices-.pdf:PDF},
  keywords      = {Condensed Matter - Quantum Gases},
  primaryclass  = {cond-mat},
}

@Article{Lee2013,
  author    = {Lee, Tony E. and Gopalakrishnan, Sarang and Lukin, Mikhail D.},
  journal   = {Physical Review Letters},
  title     = {Unconventional {Magnetism} via {Optical} {Pumping} of {Interacting} {Spin} {Systems}},
  year      = {2013},
  month     = jun,
  number    = {25},
  pages     = {257204},
  volume    = {110},
  abstract  = {We consider strongly interacting systems of effective spins, subject to dissipative spin-flip processes associated with optical pumping. We predict the existence of novel magnetic phases in the steady state of this system, which emerge due to the competition between coherent and dissipative processes. Specifically, for strongly anisotropic spin-spin interactions, we find ferromagnetic, antiferromagnetic, spin-density-wave, and staggered-XY steady states, which are separated by nonequilibrium phase transitions meeting at a Lifshitz point. These transitions are accompanied by quantum correlations, resulting in spin squeezing. Experimental implementations in ultracold atoms and trapped ions are discussed.},
  doi       = {10.1103/PhysRevLett.110.257204},
  file      = {:Lee2013.Unconventional Magnetism via-PRL.pdf:PDF},
  groups    = {XY Project},
  publisher = {American Physical Society},
  url       = {https://link.aps.org/doi/10.1103/PhysRevLett.110.257204},
  urldate   = {2021-10-08},
}

@Article{Maghrebi2016,
  author    = {Maghrebi, Mohammad F. and Gorshkov, Alexey V.},
  journal   = {Physical Review B},
  title     = {Nonequilibrium many-body steady states via {Keldysh} formalism},
  year      = {2016},
  month     = jan,
  number    = {1},
  pages     = {014307},
  volume    = {93},
  abstract  = {Many-body systems with both coherent dynamics and dissipation constitute a rich class of models which are nevertheless much less explored than their dissipationless counterparts. The advent of numerous experimental platforms that simulate such dynamics poses an immediate challenge to systematically understand and classify these models. In particular, nontrivial many-body states emerge as steady states under nonequilibrium dynamics. While these states and their phase transitions have been studied extensively with mean-field theory, the validity of the mean-field approximation has not been systematically investigated. In this paper, we employ a field-theoretic approach based on the Keldysh formalism to study nonequilibrium phases and phase transitions in a variety of models. In all cases, a complete description via the Keldysh formalism indicates a partial or complete failure of the mean-field analysis. Furthermore, we find that an effective temperature emerges as a result of dissipation, and the universal behavior including the dynamics near the steady state is generically described by a thermodynamic universality class.},
  doi       = {10.1103/PhysRevB.93.014307},
  file      = {:Maghrebi2016.Nonequilibrium many body-PRB.pdf:PDF},
  groups    = {XY Project},
  publisher = {American Physical Society},
  url       = {https://link.aps.org/doi/10.1103/PhysRevB.93.014307},
  urldate   = {2021-10-08},
}

@Article{Singh2011,
  author    = {Singh, Sukhwinder and Pfeifer, Robert N. C. and Vidal, Guifre},
  journal   = {Physical Review B},
  title     = {Tensor network states and algorithms in the presence of a global {U}(1) symmetry},
  year      = {2011},
  month     = mar,
  number    = {11},
  pages     = {115125},
  volume    = {83},
  abstract  = {Tensor network decompositions offer an efficient description of certain many-body states of a lattice system and are the basis of a wealth of numerical simulation algorithms. In a recent paper [Phys. Rev. A 82, 050301 (2010)] we discussed how to incorporate a global internal symmetry, given by a compact, completely reducible group G, into tensor network decompositions and algorithms. Here we specialize to the case of Abelian groups and, for concreteness, to a U(1) symmetry, associated, e.g., with particle number conservation. We consider tensor networks made of tensors that are invariant (or covariant) under the symmetry, and explain how to decompose and manipulate such tensors in order to exploit their symmetry. In numerical calculations, the use of U(1)-symmetric tensors allows selection of a specific number of particles, ensures the exact preservation of particle number, and significantly reduces computational costs. We illustrate all these points in the context of the multiscale entanglement renormalization Ansatz.},
  doi       = {10.1103/PhysRevB.83.115125},
  file      = {:Singh2011.Tensor network states-PRB.pdf:PDF},
  groups    = {Symmetric TN},
  publisher = {American Physical Society},
  url       = {https://link.aps.org/doi/10.1103/PhysRevB.83.115125},
  urldate   = {2021-10-08},
}

