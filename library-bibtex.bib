  keywords      = {quantum algorithms; open quantum systems; Lindblad simulation},
  language      = {en},
  local-url     = {/Users/JackDunham/OneDrive - University College London/Library/Unpublished/C/Cleve2017_Cleve;Wang_Efficient Quantum Algorithms for Simulating Lindblad Evolution__0.pdf},
  pdf           = {:Unpublished/C/Cleve2017 - Cleve, Wang - Efficient Quantum Algorithms for Simulating Lindblad Evolution -.pdf:PDF},
